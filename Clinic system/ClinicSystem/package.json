    "jsonwebtoken": "^9.0.2",
    "joi": "^17.9.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "express-rate-limit": "^6.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1",
    "moment": "^2.29.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
