{
  "name": "clinic-management-backend",
  "version": "1.0.0",
  "description": "Backend API for clinic management system with appointment scheduling, triage, and analytics",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
